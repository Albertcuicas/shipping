{
    "name": "vinnia/shipping",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vinnia AB",
            "email": "hello@vinnia.se"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "guzzlehttp/guzzle": "^6.2",
        "moneyphp/money": "^3.0",
        "vinnia/util": "dev-master"
    },
    "autoload": {
        "psr-4": {
